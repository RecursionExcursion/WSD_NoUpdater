<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.wsd.controllers.NewStartupViewController">
    <center>
        <TableView fx:id="pathTableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <top>
        <HBox BorderPane.alignment="CENTER">
            <children>
                <HBox prefHeight="25.0" prefWidth="201.0">
                    <children>
                        <Label text="Name"/>
                        <TextField fx:id="startUpNameTextField" prefHeight="25.0" prefWidth="125.0"/>
                    </children>
                </HBox>
                <Button fx:id="addRowButton" mnemonicParsing="false" onAction="#addRowClick" text="Add Row"/>
            </children>
        </HBox>
    </top>
    <bottom>
        <Button mnemonicParsing="false" onAction="#createStartUpClick" text="Create" BorderPane.alignment="CENTER"/>
    </bottom>
</BorderPane>
